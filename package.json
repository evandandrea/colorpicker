  },
  "repository": "https://github.com/toinane/colorpicker",
  "keywords": [
    "ColorPicker",
    "Toinane",
    "Crea-THAT",
    "crea-that"
  ],
  "devDependencies": {
    "electron": "^1.6.1",
    "electron-builder": "^19.45.5"
  },
  "author": "Toinane",
  "homepage": "https://toinane.crea-th.at",
