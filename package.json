{
  "name": "colorpicker",
  "productName": "colorpicker",
  "version": "1.2.2",
  "description": "Small app to get color code",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "text": "xvfb-maybe electron-mocha main.js",
